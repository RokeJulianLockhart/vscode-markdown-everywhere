{
	"injectionSelector": "L:source.coffee, L:source.dockerfile, L:text.git-commit, L:text.git-rebase, L:source.diff, L:source.ignore, L:source.ini, L:source.makefile, L:source.perl, L:source.perl.6, L:source.powershell, L:source.python, L:source.r, L:source.ruby, L:source.shell, L:source.yaml, L:source.julia",
	"patterns": [
		{
			"include": "#comment-markdown-cell-inject-number-sign-LRSM"
		}
	],
	"repository": {
		"comment-markdown-cell-inject-number-sign-LRSM": {
			"name": "comment.quote_code.number-sign.LRSM",
			"begin": "(?<=(?:^|\\G)\\s*#\\s*\\[markdown\\])$",
			"end": "(^|\\G)(?=\\s*(?!#|\\s)|\\s*$)",
			"patterns": [
				{
					"include": "#number-sign-quote-LRSM"
				}
			]
		},
		"number-sign-quote-LRSM": {
			"begin": "(^|\\G)\\s*(#) ?",
			"captures": {
				"2": {
					"name": "comment.punctuation.definition.quote_code.number-sign.LRSM"
				}
			},
			"contentName": "meta.embedded.block.md",
			"patterns": [
				{
					"include": "text.html.markdown"
				}
			],
			"while": "(^|\\G)\\s*(#) ?"
		}
	},
    "scopeName": "comment.markdown-cell-inject.number-sign.LRSM"
}