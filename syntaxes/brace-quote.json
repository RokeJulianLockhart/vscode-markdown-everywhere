{
	"injectionSelector": "L:source.haskell -string -comment -meta.embedded.block.everywhere.md, L:source.purescript -string -comment -meta.embedded.block.everywhere.md",
	"patterns": [
		{
			"include": "#comment-markdown-cell-inject-brace-quote-BR"
		}
	],
	"repository": {
		"comment-markdown-cell-inject-brace-quote-BR": {
			"begin": "(^)({-\\s*\\[markdown\\]) ?$",
			"captures": {
				"2": {
					"name": "comment.punctuation.definition.quote_code.brace-quote.BR"
				}
			},
			"name": "comment.quote_code.brace-quote.BR",
			"contentName": "meta.embedded.block.everywhere.md",
			"patterns": [
				{
					"include": "text.html.markdown"
				}
			],
			"end": "( ?)(-})"
		}
	},
    "scopeName": "comment.markdown-cell-inject.brace-quote.BR"
}