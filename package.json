{
    "name": "vscode-markdown-everywhere",
    "displayName": "Markdown Everywhere",
    "description": "Embed and highlight markdown everywhere",
    "version": "0.0.1",
    "publisher": "zhaouv",
    "license": "Apache",
    "repository": {
        "url": "https://github.com/zhaouv/vscode-markdown-everywhere.git"
    },
    "bugs": {
        "url": "https://github.com/zhaouv/vscode-markdown-everywhere/issues"
    },
    "engines": {
        "vscode": "^1.21.99"
    },
    "categories": [
        "Programming Languages"
    ],
    "keywords": [
        "markdown",
        "embed",
        "cell",
        "highlight"
    ],
    "scripts": {
        "build": "node ./build/build"
    },
    "main": "./build/extension",
    "contributes": {
        "commands": [
            {
                "command": "markdown-everywhere.showPreviewToSide",
                "title": "Preview Embedded Markdown",
                "icon": "$(open-preview)"
            }
        ],
        "markdown.markdownItPlugins": true,
        "configuration": {
            "type": "object",
            "title": "Markdown Everywhere",
            "properties": {
                "markdown-everywhere.enhancing-typing": {
                    "type": "boolean",
                    "default": true,
                    "description": "Inject onEnterRules to markdown (reload needed). After opening, extra characters will be added when editing markdown and pressing enter."
                },
                "markdown-everywhere.preview-mode": {
                    "type": "string",
                    "default": "splitter",
                    "enum": [
                        "splitter",
                        "ignored",
                        "fenced"
                    ],
                    "description": "How to deal with the original source code (re-preview to trigger). 'splitter' for ignoring them and put lines as splitter. 'ignore' for ignoring. 'fenced' for showing source code as fenced code."
                }
            }
        },
        "menus": {
            "editor/title": [
                {
                    "command": "markdown-everywhere.showPreviewToSide",
                    "when": "editorLangId != markdown && !notebookEditorFocused",
                    "group": "navigation"
                }
            ],
            "commandPalette": [
                {
                    "command": "markdown-everywhere.showPreviewToSide",
                    "when": "editorLangId != markdown && !notebookEditorFocused",
                    "group": "navigation"
                }
            ]
        },
        "grammars": [
            {
                "injectTo": [
                    "source.python"
                ],
                "scopeName": "comment.markdown-cell-inject.triple-quote.BR",
                "path": "./syntaxes/triple-quote.json",
                "embeddedLanguages": {
                    "meta.embedded.block.everywhere.md": "markdown"
                }
            },
            {
                "injectTo": [
                    "source.coffee",
                    "source.dockerfile",
                    "text.git-commit",
                    "text.git-rebase",
                    "source.diff",
                    "source.ignore",
                    "source.ini",
                    "source.makefile",
                    "source.perl",
                    "source.perl.6",
                    "source.powershell",
                    "source.python",
                    "source.r",
                    "source.ruby",
                    "source.shell",
                    "source.yaml",
                    "source.cython",
                    "source.julia",
                    "source.cmake"
                ],
                "scopeName": "comment.markdown-cell-inject.number-sign-MD.LRSW",
                "path": "./syntaxes/number-sign-MD.json",
                "embeddedLanguages": {
                    "meta.embedded.block.everywhere.md": "markdown"
                }
            },
            {
                "injectTo": [
                    "source.coffee",
                    "source.dockerfile",
                    "text.git-commit",
                    "text.git-rebase",
                    "source.diff",
                    "source.ignore",
                    "source.ini",
                    "source.makefile",
                    "source.perl",
                    "source.perl.6",
                    "source.powershell",
                    "source.python",
                    "source.r",
                    "source.ruby",
                    "source.shell",
                    "source.yaml",
                    "source.cython",
                    "source.julia",
                    "source.cmake"
                ],
                "scopeName": "comment.markdown-cell-inject.number-sign.LRSM",
                "path": "./syntaxes/number-sign.json",
                "embeddedLanguages": {
                    "meta.embedded.block.everywhere.md": "markdown"
                }
            },
            {
